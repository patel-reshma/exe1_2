CC = gcc
EXE = TicketReservation 

all: clean binary

binary: main.o product.o 
	$CC main.o product.o
clean :
	@echo "\033[0;31m"
	@echo "========================================================="
	@echo "                 Cleaning Started                    "
	@echo "========================================================="
	@echo "\033[0m"
	
	rm  -rf *.o *~
	
	@echo "\033[0;31m"
	@echo "========================================================="
	@echo "                 Cleaning Finished                       "
	@echo "========================================================="
	@echo "\033[0m"


